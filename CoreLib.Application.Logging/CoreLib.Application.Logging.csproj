<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>peteralmanzar</Authors>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <Description>Provides simple and extensible framework for application logging</Description>
    <PackageTags>simple application logging framework log</PackageTags>
    <Copyright>2019</Copyright>
    <PackageProjectUrl>https://github.com/peteralmanzar/CoreLib.Application.Logging</PackageProjectUrl>
    <RepositoryUrl>https://github.com/peteralmanzar/CoreLib.Application.Logging</RepositoryUrl>
    <RepositoryType>github</RepositoryType>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="System.Diagnostics.EventLog" Version="4.5.0" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
